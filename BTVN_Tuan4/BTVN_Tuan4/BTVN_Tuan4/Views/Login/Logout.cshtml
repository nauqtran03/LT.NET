
@{
    ViewBag.Title = "Logout";
}

<h2>Logout</h2>
<form method="post">
    <div>
        <label>Username : </label>
        <input type="type" name="username"  required/>
    </div>
    <div>
        <label>Password : </label>
        <input type="password" name="password" required />
    </div>
    <input type="submit" value="Đăng nhập" />
</form>

@if(ViewBag.msg != null)
            {
                <p>@ViewBag.msg</p>
            }